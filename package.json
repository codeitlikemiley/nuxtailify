{
  "name": "nuxtailify",
  "version": "1.0.0",
  "description": "Nuxt Tailwind and Vuetify Starter Web App",
  "author": "codeitlikemiley",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:eslint": "eslint --fix",
    "lint:stylelint": "stylelint --fix",
    "lint:markdownlint": "markdownlint",
    "lint:prettier": "prettier --write --loglevel warn",
    "lint:all:eslint": "yarn lint:eslint --ext .js,.vue .",
    "lint:all:stylelint": "yarn lint:stylelint \"src/**/*.{vue,scss}\"",
    "lint:all:markdownlint": "yarn lint:markdownlint \"docs/*.md\" \"*.md\"",
    "lint:all:prettier": "yarn lint:prettier \"**/*.{js,json,css,scss,vue,html,md}\"",
    "lint": "run-s lint:all:*",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.0.0",
    "nuxt": "^2.0.0",
    "nuxt-purgecss": "^0.2.0",
    "tailwindcss": "^0.7.3",
    "vuetify": "^1.3.15",
    "vuetify-loader": "^1.0.8"
  },
  "devDependencies": {
    "autoprefixer": "^9.4.3",
    "babel-eslint": "^8.2.1",
    "cross-env": "^5.2.0",
    "cssnano": "^4.1.8",
    "eslint": "^5.0.1",
    "eslint-config-prettier": "^3.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-vue": "^4.0.0",
    "@fortawesome/fontawesome-free": "^5.6.1",
    "material-design-icons-iconfont": "^4.0.3",
    "nodemon": "^1.11.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.5.0",
    "postcss-url": "^8.0.0",
    "prettier": "1.14.3",
    "roboto-fontface": "^0.10.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2"
  }
}
